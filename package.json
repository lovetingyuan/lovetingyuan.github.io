{
  "name": "tingyuan",
  "private": true,
  "description": "庭院的个人网站",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://lovetingyuan.github.io",
  "scripts": {
    "dev": "vite",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "build": "cross-env NODE_ENV=production && npm run build:ssr && npm run build:client",
    "build:client": "cross-env NODE_ENV=production && vite build",
    "build:ssr": "cross-env NODE_ENV=production vite build --ssr src/server.ts --outDir dist-ssr",
    "serve": "vite preview",
    "deploy": "node ./scripts/deploy.cjs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "knip": "npx -y knip@latest -c scripts/knip.js"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.1",
    "github-markdown-css": "^5.5.0",
    "iconify-icon": "^2.0.0",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@happy-dom/global-registrator": "^13.3.8",
    "@rushstack/eslint-patch": "^1.7.2",
    "@shikijs/markdown-it": "^1.1.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/markdown-it-container": "^2.0.9",
    "@types/markdown-it-link-attributes": "^3.0.4",
    "@types/node": "^18.18.10",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-vue": "^9.18.1",
    "knip": "^5.0.1",
    "markdown-it-container": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "piscina": "^4.2.1",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.3.0",
    "unplugin-vue-markdown": "^0.25.2",
    "vite": "^5.1.1",
    "vite-plugin-pwa": "^0.18.1",
    "vue-tsc": "^1.8.27",
    "workbox-build": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}
