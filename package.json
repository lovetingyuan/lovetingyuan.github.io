{
  "name": "tingyuan",
  "private": true,
  "description": "庭院的个人网站",
  "version": "0.0.0",
  "homepage": "https://lovetingyuan.github.io",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "type-check": "vue-tsc --noEmit --composite false",
    "build": "cross-env NODE_ENV=production && npm run build:ssr && npm run build:client",
    "build:client": "cross-env NODE_ENV=production && vite build",
    "build:ssr": "cross-env NODE_ENV=production vite build --ssr src/server.ts --outDir dist-ssr",
    "serve": "vite preview",
    "deploy": "node ./scripts/deploy",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^10.1.0",
    "balloon-css": "^1.2.0",
    "github-markdown-css": "^5.2.0",
    "normalize.css": "^8.0.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.2",
    "@happy-dom/global-registrator": "^9.9.2",
    "@iconify-json/material-symbols": "^1.1.40",
    "@iconify-json/mdi": "^1.1.46",
    "@rushstack/eslint-patch": "^1.2.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@tsconfig/node18": "^2.0.0",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-container": "^2.0.5",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "markdown-it": "^13.0.1",
    "markdown-it-container": "^3.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.8.1",
    "piscina": "^3.2.0",
    "prettier": "^2.8.4",
    "typescript": "~5.0.4",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.3.9",
    "vite-plugin-pwa": "^0.14.4",
    "vite-plugin-vue-markdown": "^0.22.6",
    "vue-tsc": "^1.4.4",
    "workbox-build": "^6.5.4",
    "workbox-core": "^6.5.4",
    "workbox-precaching": "^6.5.4",
    "workbox-routing": "^6.5.4",
    "workbox-strategies": "^6.5.4",
    "workbox-window": "^6.5.4"
  }
}
