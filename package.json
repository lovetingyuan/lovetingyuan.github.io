{
  "name": "tingyuan",
  "private": true,
  "description": "庭院的个人网站",
  "version": "0.0.0",
  "homepage": "https://lovetingyuan.github.io",
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "build": "cross-env NODE_ENV=production && npm run build:ssr && npm run build:client",
    "build:client": "cross-env NODE_ENV=production && vite build",
    "build:ssr": "cross-env NODE_ENV=production vite build --ssr src/server.ts --outDir dist-ssr",
    "serve": "vite preview",
    "deploy": "node ./scripts/deploy",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "knip": "knip -c scripts/knip.js"
  },
  "dependencies": {
    "@vueuse/core": "^10.6.1",
    "github-markdown-css": "^5.4.0",
    "iconify-icon": "^1.0.8",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@happy-dom/global-registrator": "^12.10.3",
    "@rushstack/eslint-patch": "^1.5.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/markdown-it-container": "^2.0.9",
    "@types/markdown-it-link-attributes": "^3.0.4",
    "@types/node": "^18.18.10",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.54.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "knip": "^2.41.3",
    "markdown-it-container": "^3.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "piscina": "^4.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.1.6",
    "unplugin-vue-markdown": "^0.25.2",
    "vite": "^5.0.5",
    "vite-plugin-pwa": "^0.17.0",
    "vue-tsc": "^1.8.22",
    "workbox-build": "^7.0.0",
    "workbox-window": "^7.0.0"
  }
}
