{
  "name": "tingyuan",
  "private": true,
  "description": "庭院的个人网站",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://lovetingyuan.github.io",
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --build --force",
    "build": "npm run build:ssr && npm run build:client",
    "build:client": "cross-env NODE_ENV=production && vite build",
    "build:ssr": "cross-env NODE_ENV=production vite build --ssr src/server.ts --outDir dist-ssr",
    "serve": "vite preview",
    "deploy": "node ./scripts/deploy.cjs",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/ scripts/ blogs/ public/",
    "preview": "vite preview",
    "knip": "npx -y knip@latest -c scripts/knip.ts"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "github-markdown-css": "^5.5.0",
    "iconify-icon": "^2.1.0",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@happy-dom/global-registrator": "^14.12.0",
    "@iconify/vue": "^4.1.2",
    "@rushstack/eslint-patch": "^1.10.2",
    "@shikijs/markdown-it": "^1.9.1",
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/markdown-it-container": "^2.0.9",
    "@types/markdown-it-link-attributes": "^3.0.4",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "daisyui": "^4.12.10",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-sonarjs": "^0.25.1",
    "eslint-plugin-vue": "^9.26.0",
    "html-minifier-terser": "^7.2.0",
    "knip": "^5.23.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.0",
    "tinypool": "^1.0.0",
    "typescript": "~5.4.5",
    "unplugin-detect-duplicated-deps": "^2.0.0",
    "unplugin-vue-markdown": "^0.26.0",
    "vite": "^5.3.0",
    "vite-plugin-pwa": "^0.20.0",
    "vue-tsc": "^2.0.14",
    "workbox-build": "^7.1.0",
    "workbox-window": "^7.1.0"
  },
  "simple-git-hooks": {
    "pre-push": "node scripts/prebuild.cjs"
  }
}
