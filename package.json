{
  "name": "tingyuan",
  "version": "0.0.0",
  "private": true,
  "description": "庭院的个人网站",
  "homepage": "https://lovetingyuan.github.io",
  "type": "module",
  "scripts": {
    "build": "npm run build:ssr && npm run build:client",
    "build:client": "cross-env NODE_ENV=production && vite build",
    "build:ssr": "cross-env NODE_ENV=production vite build --ssr src/server.ts --outDir dist-ssr",
    "deploy": "node ./scripts/deploy",
    "dev": "vite",
    "format": "prettier . --write",
    "knip": "npx -y knip@latest -c scripts/knip.ts",
    "lint": "eslint . --fix",
    "preview": "vite preview",
    "serve": "vite preview",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@vueuse/core": "^13.5.0",
    "github-markdown-css": "^5.8.1",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@happy-dom/global-registrator": "^18.0.1",
    "@iconify/vue": "^5.0.0",
    "@shikijs/markdown-it": "^3.7.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@tsconfig/node20": "^20.1.6",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/markdown-it-container": "^2.0.10",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^20.19.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "daisyui": "^5.0.43",
    "eslint": "^9.30.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.4",
    "eslint-plugin-vue": "^10.3.0",
    "html-minifier-terser": "^7.2.0",
    "knip": "^5.61.3",
    "markdown-it-container": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.18",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tinypool": "^2.0.0",
    "typescript": "~5.4.5",
    "typescript-eslint": "^8.35.1",
    "unplugin-detect-duplicated-deps": "^2.0.0",
    "unplugin-vue-markdown": "^29.1.0",
    "vite": "^7.0.2",
    "vite-plugin-pwa": "^1.0.1",
    "vue-tsc": "^3.0.1",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  }
}
