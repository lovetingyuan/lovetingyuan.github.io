{
  "name": "tingyuan",
  "private": true,
  "description": "庭院的个人网站",
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://lovetingyuan.github.io",
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --build --force",
    "build": "npm run build:ssr && npm run build:client",
    "build:client": "cross-env NODE_ENV=production && vite build",
    "build:ssr": "cross-env NODE_ENV=production vite build --ssr src/server.ts --outDir dist-ssr",
    "serve": "vite preview",
    "deploy": "node ./scripts/deploy",
    "lint": "eslint . --fix",
    "format": "prettier . --write",
    "preview": "vite preview",
    "knip": "npx -y knip@latest -c scripts/knip.ts"
  },
  "dependencies": {
    "@vueuse/core": "^11.1.0",
    "github-markdown-css": "^5.7.0",
    "iconify-icon": "^2.1.0",
    "vue": "^3.5.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "@happy-dom/global-registrator": "^15.7.3",
    "@iconify/vue": "^4.1.2",
    "@rushstack/eslint-patch": "^1.10.2",
    "@shikijs/markdown-it": "^1.22.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/markdown-it-container": "^2.0.9",
    "@types/markdown-it-link-attributes": "^3.0.4",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "daisyui": "^4.12.10",
    "eslint": "^9.13.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^2.0.4",
    "eslint-plugin-vue": "^9.29.1",
    "globals": "^15.11.0",
    "html-minifier-terser": "^7.2.0",
    "knip": "^5.23.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-link-attributes": "^4.0.1",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "tailwindcss": "^3.4.0",
    "tinypool": "^1.0.0",
    "typescript": "~5.4.5",
    "unplugin-detect-duplicated-deps": "^2.0.0",
    "unplugin-vue-markdown": "^0.26.0",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^0.20.0",
    "vue-tsc": "^2.1.4",
    "workbox-build": "^7.1.0",
    "workbox-window": "^7.1.0"
  }
}
